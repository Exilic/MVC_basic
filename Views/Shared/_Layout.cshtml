<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="~/css/site.css" />
    <title>@ViewData["Title"] - TestMVCBasic</title>
</head>
<body>
    <header>
        <img src="~/img/logo.svg" width="60" />
        <ul>
            <li>@Html.ActionLink("Index", "Index", "Home")</li>
            <li>@Html.ActionLink("About", "Index", "About")</li>
            <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
            <li>@Html.ActionLink("Projects", "Index", "Projects")</li>
        </ul>
    </header>
    <main>
        @RenderBody()
    </main>
    <footer><p>@DateTime.Today.ToString("D") /   Tobias Engberg</p></footer>
</body>
</html>
